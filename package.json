{
  "name": "serverless-crawler",
  "version": "1.0.0",
  "description": "Distributed serverless web crawling/web scraping with support for js execution, proxying, and autoscaling",
  "main": "lib/fn.js",
  "bin": {
    "cli": "bin/cli.js"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "scripts": {
    "test": "mocha --exit --grep @remote --invert --timeout 10000 './{,!(node_modules)/**/}*.spec.js'",
    "test:start": "docker-compose -f ./test/docker-compose.yml up -d",
    "test:stop": "docker-compose -f ./test/docker-compose.yml down",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Ani Channarasappa",
  "license": "MIT",
  "devDependencies": {
    "@hapi/hapi": "^18.3.1",
    "@hapi/vision": "^5.5.2",
    "docker-names": "^1.1.1",
    "handlebars": "^4.1.2",
    "mocha": "^5.2.0",
    "prettyjson": "^1.2.1",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.3"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "draftlog": "^1.0.12",
    "inquirer": "^6.2.1",
    "ioredis": "^4.3.0",
    "joi": "^14.3.1",
    "moment": "^2.24.0",
    "puppeteer": "^1.11.0",
    "ramda": "^0.26.1",
    "yargs": "^12.0.5"
  }
}
